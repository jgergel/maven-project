pipeline {
	agent any
	stages {
		stage('Build'){
			steps {
				sh '/apps/mvn/bin/mvn package'
//				build job: 'JenkinsClassMvnPackage'
			}
// 			post {
//				success {
//					echo 'Now Archiving...'
//					archiveArtifacts artifacts: '**/target/*.war'
//				}
//			}
		}
//
		stage('Policy Evaluation'){
			steps {
				nexusPolicyEvaluation iqApplication: 'JenkinsClassHello', iqStage: 'build'
			}
		}
		stage ('Deploy to Staging') {
			steps {
				build job: 'JenkinsClassDeployToStage'
			}
		}
	}
}
	
