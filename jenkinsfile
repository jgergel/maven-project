pipeline {
	agent any
	stages {
		stage('Build'){
			steps {
				sh 'mvn clean package'
			}
			post {
				success {
					echo 'Now Archiving...'
					archiveArtifacts artifices: '**/target/*.war'
				}
			}
		}
		stage ('Deploy to Staging')
			steps {
				build job: 'JenkinsClassDeployToStage'
			}
		}
	}
}
	
